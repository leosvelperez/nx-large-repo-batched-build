{
  "version": 2,
  "projects": {
    "ng-build": {
      "root": "libs/ng-build",
      "sourceRoot": "libs/ng-build/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "build/libs/ng-build",
            "tsConfig": "libs/ng-build/tsconfig.lib.json",
            "packageJson": "libs/ng-build/package.json",
            "main": "libs/ng-build/src/index.ts",
            "assets": [
              "libs/ng-build/*.md",
              {
                "input": "./libs/ng-build/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/ng-build",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ng-build/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ng-build"],
          "options": {
            "jestConfig": "libs/ng-build/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
